{
  "require": {
    "aelvan/stamp": "^2.1",
    "carlcs/craft-redactorcustomstyles": "^3.0",
    "clubstudioltd/craft-asset-rev": "^6.0",
    "craftcms/cms": "^3.0.0",
    "craftcms/contact-form": "^2.2",
    "craftcms/contact-form-honeypot": "^1.0",
    "craftcms/element-api": "^2.6",
    "craftcms/redactor": "^2.4",
    "ether/seo": "^3.6",
    "lucasbares/craft-emailobfuscator": "^2.1",
    "pennebaker/craft-architect": "^2.3",
    "utakka/redactor-anchors": "^1.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
